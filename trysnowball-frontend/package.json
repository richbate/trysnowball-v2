{
  "name": "trysnowball-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@supabase/auth-js": "^2.71.1",
    "@supabase/supabase-js": "^2.52.1",
    "jsdom": "^26.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx",
    "validate-csp": "node scripts/validate-csp.js",
    "prebuild": "npm run show-git-info",
    "show-git-info": "echo '=== GIT INFORMATION ===' && git log --oneline -5 && echo 'Current branch:' && git branch --show-current && echo 'Latest commit:' && git rev-parse HEAD && echo '=== END GIT INFO ==='"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
